<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>sdiff</key>
	<string>sdiff -l --strip-trailing-cr $1 $2</string>
	<key>FileMerge</key>
	<string>opendiff $1 $2</string>
	<key>BBEdit</key>
	<string>/usr/local/bin/bbdiff $1 $2</string>
</dict>
</plist>
